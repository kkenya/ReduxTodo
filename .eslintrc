{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "no-underscore-dangle": [
      2,
      {
        "allowAfterThis": true
      }
    ],
    "react/prop-types": [
      0
    ],
    "object-curly-newline": [
      0
    ]
  }
}